{
  "name": "wetube",
  "version": "1.0.0",
  "description": "Cloning Youtube with Vanila and NodeJS",
  "main": "init.js",
  "author": "seyyyg",
  "dependencies": {
    "@babel/core": "^7.8.4",
    "@babel/node": "^7.8.4",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.8.4",
    "autoprefixer": "^9.8.0",
    "axios": "^0.19.2",
    "babel-loader": "^8.1.0",
    "body-parser": "^1.19.0",
    "connect-mongo": "^3.2.0",
    "cookie-parser": "^1.4.4",
    "css-loader": "^3.5.3",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.10.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "get-blob-duration": "^1.1.1",
    "helmet": "^3.21.2",
    "mongoose": "^5.9.3",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "node-sass": "^4.14.1",
    "passport": "^0.4.1",
    "passport-facebook": "^3.0.0",
    "passport-github": "^1.1.0",
    "passport-local": "^1.0.0",
    "passport-local-mongoose": "^6.0.1",
    "postcss-loader": "^3.0.0",
    "sass-loader": "^8.0.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "scripts": {
    "dev:server": "nodemon --exec babel-node init.js --delay 2",
    "dev:assets": "cd src && WEBPACK_ENV=development webpack -w",
    "build:assets": "cd src && WEBPACK_ENV=production webpack",
    "build:server":"babel src --out-dir build --ignore 'src/assets', 'src/statics', 'src/webpack.config.js'",
    "build":"npm run build:server && npm run build:assets",
    "prebuild": "rm -rf build",
    "tunnel": "lt --port 4000"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.2.1",
    "nodemon": "^2.0.2"
  }
}
